    "@react-navigation/stack": "^6.3.17",
    "@supabase/supabase-js": "^2.38.4",
    "expo": "^51.0.14",
    "expo-font": "~12.0.7",
    "expo-linear-gradient": "^13.0.2",
    "expo-splash-screen": "~0.27.5",
    "expo-status-bar": "~1.12.1",
    "expo-system-ui": "~3.0.6",
